{
  "name": "premium-app-marketplace",
  "version": "1.0.0",
  "description": "Microservice-based premium application marketplace",
  "scripts": {
    "start:gateway": "cd services/api-gateway && npm start",
    "start:auth": "cd services/auth-service && npm start",
    "start:product": "cd services/product-service && npm start",
    "start:order": "cd services/order-service && npm start",
    "start:payment": "cd services/payment-service && npm start",
    "start:user": "cd services/user-service && npm start",
    "install:all": "npm install && cd services/api-gateway && npm install && cd ../auth-service && npm install && cd ../product-service && npm install && cd ../order-service && npm install && cd ../payment-service && npm install && cd ../user-service && npm install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "premium-apps",
    "marketplace"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2"
  }
}
